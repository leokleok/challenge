<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Hotels in Singapore</title>
  </head>
  <body>

    <h1> Hotels </h1>

    <% for(var i=0; i<hotels.length; i++) {%>
      <div>
        <h2> <%= hotels[i].name %> </h2>
        <address> <%= hotels[i].address %></address>
        <img src="<%= hotels[i].image %>"/>
        <% if (hotels[i].minPrice === null){ %>
          <p> All Rooms Booked </p>
        <%} else  { %>
          <p> Price: <%= hotels[i].minPrice %></p>
          <p> Source: <%= hotels[i].minPriceSource %></p>
        <% } %>


      </div>
    <% } %>

  </body>
</html>
